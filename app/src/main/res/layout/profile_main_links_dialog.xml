<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <Button
        android:id="@+id/profile_dialog_instagram"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Instagram"
        app:layout_constraintBaseline_toBaselineOf="@+id/profile_dialog_github"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/profile_dialog_github"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="GitHub"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/profile_dialog_linkedin"
        app:layout_constraintStart_toEndOf="@+id/profile_dialog_instagram"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/profile_dialog_linkedin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="Linkedin"
        app:layout_constraintBaseline_toBaselineOf="@+id/profile_dialog_github"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>